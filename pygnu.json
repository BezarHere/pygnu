{
    "output_dir": ".",
    "output_cache_dir": ".",
    "source_selectors": [
        "**/*.c",
        "**/*.cpp",
        "**/*.cc",
        "**/*.cxx"
    ],
    "build_configurations": {
        "debug": {
            "predefines": {
                "_DEBUG": null
            },
            "optimization_lvl": 2,
            "optimization_type": 2,
            "standard": 3,
            "warning_level": 2,
            "warning_pedantic": false,
            "print_includes": false,
            "catch_typos": true,
            "exit_on_errors": true,
            "dynamicly_linkable": true,
            "print_stats": true,
            "include_dirs": [],
            "lib_dirs": {},
            "lib_names": [],
            "assempler_args": [],
            "linker_args": [],
            "preprocessor_args": []
        },
        "release": {
            "predefines": {
                "NDEBUG": null,
                "_RELEASE": null
            },
            "optimization_lvl": 4,
            "optimization_type": 4,
            "standard": 3,
            "warning_level": 2,
            "warning_pedantic": false,
            "print_includes": false,
            "catch_typos": true,
            "exit_on_errors": true,
            "dynamicly_linkable": true,
            "print_stats": true,
            "include_dirs": [],
            "lib_dirs": {},
            "lib_names": [],
            "assempler_args": [],
            "linker_args": [],
            "preprocessor_args": []
        }
    }
}